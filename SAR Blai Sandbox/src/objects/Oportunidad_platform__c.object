    <webLinks>
        <fullName>Enviar_a_tablet</fullName>
        <availability>online</availability>
        <description>Botón utilizando para enviar el archivo PDF a la tableta.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Enviar a tablet</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}


var result = sforce.connection.query(&quot;SELECT Utiliza_firma_biometrica__c, userVidsigner__c, passVidsigner__c, Tableta_1__c, Tableta_2__c, Tableta_3__c FROM ACCOUNT WHERE Name = &apos;{!Oportunidad_platform__c.Centro2__c}&apos; LIMIT 1&quot;);
var records = result.getArray(&quot;records&quot;);
var box;

var createLoadingScreen = function(){
	if(box==null){
		box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
		var img = &quot;&lt;img  src=&apos;http://dc721.4shared.com/img/7s4jrCO5/s7/131b386c820/LoadingAnimation&apos; &quot;+
		&quot;data-link=&apos;http://dc721.4shared.com/img/7s4jrCO5/s7/131b386c820/LoadingAnimation&apos;&gt;&quot;;
		var mybotton = &quot;&lt;button class=&apos;btn&apos;  onclick=&apos;window.parent.box.hide(); return false;&apos;&gt;Ocultar&lt;/button&gt;&quot;;
		var content = &quot;&lt;apex:page standardController=&apos;Account&apos;&gt;&lt;h1&gt;Enviando contrato...&lt;/h1&gt;&lt;br/&gt;&lt;/apex:page&gt;&quot;;
		parent.box = box; 
		//box.setTitle(&quot;Enviando contrato&quot;); 
		box.createDialog(); 
		box.setWidth(350); 
		box.setContentInnerHTML(content); 
		box.setupDefaultButtons(); 
		box.show();
	}
};

var execution = function(idTablet){
	var result = sforce.apex.execute(&quot;CalloutVidsigner&quot;,&quot;sendContract&quot;, 	
		{opId:&quot;{!Oportunidad_platform__c.Id}&quot;, idTablet: idTablet}); 
	box.hide();
	if(result!=&quot;OK&quot;){alert(&quot;Error al enviar el contrato\n&quot; + result);} 
	else {alert(&quot;Contrato enviado&quot;); window.location.reload();} 		
};



if(records[0].Utiliza_firma_biometrica__c == &quot;true&quot;){

	if(&quot;{!Oportunidad_platform__c.Enviado_a_signar__c}&quot; == 0){
		
		if(&quot;{!Oportunidad_platform__c.Etapa__c}&quot; == &quot;Ingreso&quot; ){
			if(records[0].userVidsigner__c!=null &amp;&amp; records[0].passVidsigner__c!=null){
			
				var numTablets = 0;
				var idTablet = 0;
				if(records[0].Tableta_1__c!=null){
					numTablets++;
					idTablet = records[0].Tableta_1__c;
				}if(records[0].Tableta_2__c!=null){
					numTablets++;
					idTablet = records[0].Tableta_2__c;
				}if(records[0].Tableta_3__c!=null){
					numTablets++;
					idTablet = records[0].Tableta_3__c;
				}

				if(numTablets==0){
					alert(&quot;No hay ninguna tablet asignada a este centro&quot;);
					
				}else if(numTablets==1){
					createLoadingScreen();
					setTimeout(function(){ execution(idTablet);}, 0);
					
				}else{
					//window.open(&quot;/apex/SeleccionarTablet?Id={!Oportunidad_platform__c.Id}&quot;, &apos;_blank&apos;);	
					window.location.href =  &quot;/apex/SeleccionarTablet?Id={!Oportunidad_platform__c.Id}&quot;				
				}
			}else{
				alert(&quot;Falta el user/pass de acceso a VidSigner&quot;);
			}	
			
		}else {
			alert (&quot;La etapa tiene que ser Ingreso&quot;);
		}

	}else {
		alert(&quot;El contrato ya se ha enviado&quot;);
	}
}else{
	alert(&quot;El centro no utiliza firma biométrica&quot;);
}</url>
