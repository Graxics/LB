                 (&quot;{!Oportunidad_platform__c.NOC_11_Centro__c}&quot; != &apos;&apos; || 
                  &quot;{!Oportunidad_platform__c.NOC_12_Convenios__c}&quot; != &apos;&apos; || 
                  &quot;{!Oportunidad_platform__c.NOC__c}&quot; != &apos;&apos; || 
