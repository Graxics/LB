    <validationRules>
        <fullName>Bloquear_NOC_aprobada</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Estado_Flujo__c , &quot;Aprobado&quot;)  &amp;&amp;  $Profile.Id  &lt;&gt; &apos;00eb0000000zplj&apos; &amp;&amp;  (ISCHANGED( Name ) ||  ISCHANGED( Descripcion__c ) ||  ISCHANGED(  Descuento_Fundacion__c  ) ||  ISCHANGED( Tipo__c ) ||  ISCHANGED( Alcance__c ) ||   ISCHANGED( Aspectos_economicos__c ) ||  ISCHANGED( Aspectos_no_economicos__c ) ||  ISCHANGED( Fecha_inicio__c ) ||  ISCHANGED( Fecha_fin__c ) ||  ISCHANGED( Descuento_permanente__c ) ||  ISCHANGED( Oportunidad_renegociada__c ) ||  ISCHANGED( Condiciones_Especiales__c ) ||  ISCHANGED( Oportunidad__c ) ||  ISCHANGED( Cliente__c ) ||  ISCHANGED( Descenso_de_Ocupacion__c ) ||  ISCHANGED( Ratio_de_ocupacion_actual__c ) ||  ISCHANGED( Diferencial_de_tarifas__c ) ||  ISCHANGED( Tarifas_Competencia__c ) ||  ISCHANGED( Otros_motivos__c ) ||  ISCHANGED( Descuento_aplicado__c ) ||  ISCHANGED( Precio_propuesto__c )  ||   ISCHANGED( No_se_solicita_fianza__c ))</errorConditionFormula>
        <errorMessage>No está permitido modificar una NOC aprobada</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Caducitat_Nocs_Renegociades</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName =&apos;NOC_11_Descuento_Personal_Renegociado&apos; &amp;&amp;  NOT(ISPICKVAL( Estado_Flujo__c , &apos;Aprobado&apos;))  &amp;&amp;
 ( (NOT(ISNULL(Fecha_inicio__c   )) &amp;&amp; NOT(ISNULL(CreatedDate)) &amp;&amp; Fecha_inicio__c   &lt;  DATE( YEAR( DATEVALUE(CreatedDate) ) ,1,1) &amp;&amp; DATEVALUE(CreatedDate) &gt; DATE (YEAR( DATEVALUE(CreatedDate) ),1,31)) || 
 (NOT(ISNULL(Fecha_inicio__c   )) &amp;&amp; NOT(ISNULL(CreatedDate)) &amp;&amp; DATEVALUE(CreatedDate) &lt; DATE (YEAR( DATEVALUE(CreatedDate) ),2,1) &amp;&amp; DATEVALUE(CreatedDate) &gt; DATE (YEAR( DATEVALUE(CreatedDate) ),1,1) &amp;&amp; Fecha_inicio__c   &lt;  DATE( YEAR(DATEVALUE(CreatedDate))-1 ,12,1)) ||
  (NOT(ISNULL(Fecha_Inicio_1__c   )) &amp;&amp; NOT(ISNULL(CreatedDate)) &amp;&amp; Fecha_Inicio_1__c   &lt;  DATE( YEAR( DATEVALUE(CreatedDate) ) ,1,1) &amp;&amp; DATEVALUE(CreatedDate) &gt; DATE (YEAR( DATEVALUE(CreatedDate) ),1,31)) || 
 (NOT(ISNULL(Fecha_Inicio_1__c   )) &amp;&amp; NOT(ISNULL(CreatedDate)) &amp;&amp; DATEVALUE(CreatedDate) &lt; DATE (YEAR( DATEVALUE(CreatedDate) ),2,1) &amp;&amp; DATEVALUE(CreatedDate) &gt; DATE (YEAR( DATEVALUE(CreatedDate) ),1,1) &amp;&amp; Fecha_Inicio_1__c   &lt;  DATE( YEAR( DATEVALUE(CreatedDate) )-1 ,12,1))) ||
 ( (NOT(ISNULL(Fecha_inicio__c   )) &amp;&amp; ISNULL(CreatedDate) &amp;&amp; Fecha_inicio__c   &lt;  DATE( YEAR( TODAY() ) ,1,1) &amp;&amp; TODAY()  &gt; DATE (YEAR( TODAY()  ),1,31)) || 
 (NOT(ISNULL(Fecha_inicio__c   )) &amp;&amp; ISNULL(CreatedDate) &amp;&amp; TODAY()  &lt; DATE (YEAR( TODAY()  ),2,1) &amp;&amp; TODAY()  &gt; DATE (YEAR( TODAY()  ),1,1) &amp;&amp; Fecha_inicio__c   &lt;  DATE( YEAR(TODAY() )-1 ,12,1)) ||
  (NOT(ISNULL(Fecha_Inicio_1__c   )) &amp;&amp; ISNULL(CreatedDate) &amp;&amp; Fecha_Inicio_1__c   &lt;  DATE( YEAR( TODAY()  ) ,1,1) &amp;&amp; TODAY()  &gt; DATE (YEAR( TODAY() ),1,31)) || 
 (NOT(ISNULL(Fecha_Inicio_1__c   )) &amp;&amp; ISNULL(CreatedDate) &amp;&amp; TODAY()  &lt; DATE (YEAR( TODAY()  ),2,1) &amp;&amp; TODAY()  &gt; DATE (YEAR( TODAY()  ),1,1) &amp;&amp; Fecha_Inicio_1__c   &lt;  DATE( YEAR( TODAY()  )-1 ,12,1)))</errorConditionFormula>
        <errorMessage>No puede haber una NOC renegociada con una fecha de inicio de un año anterior a la fecha de creación de la NOC</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Descuento_Fianza_no_valida</fullName>
        <active>true</active>
        <errorConditionFormula>Fianza_a_cobrar__c &gt;1 || Fianza_a_cobrar__c &lt; 0 ||  Importe_Fianza_a_cobrar__c &gt; Tarifa_oficial__c || Importe_Fianza_a_cobrar__c &lt;0</errorConditionFormula>
        <errorMessage>La fianza no es válida</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Descuento_fijo_o_por_meses</fullName>
        <active>true</active>
        <errorConditionFormula>(NOT(ISBLANK( Precio_propuesto__c )) ||  NOT(ISBLANK( Descuento_aplicado__c ))) &amp;&amp;  (NOT( ISBLANK( Importe_Mes_1__c ) )  ||  NOT(ISBLANK( Descuento_Mes_1__c )))</errorConditionFormula>
        <errorMessage>No se permite informar descuento fijo y por meses en la misma NOC</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estancia_no_puede_cambiar_en_NOC11</fullName>
        <active>true</active>
        <description>NOC 11 Descuento Personal Renegociado no pueden cambiar de tipo de estancia. Se debe abrir una nueva oportunidad</description>
        <errorConditionFormula>AND(
	RecordType.Name = &apos;NOC 11 Descuento Personal Renegociado&apos;,
	AND (NOT(ISBLANK( Tipo_estancia2__c )),
      	NOT(ISBLANK( Tipo_de_estancia_NT__c )),
      	ISCHANGED( Tipo_estancia2__c ),
      	Tipo_estancia2__c  &lt;&gt; Tipo_de_estancia_NT__c
    )
)</errorConditionFormula>
        <errorMessage>Para cambios de tipo de estancia se debe abrir una nueva oportunidad</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fechas_Importes</fullName>
        <active>true</active>
        <errorConditionFormula>Fecha_Fin_1__c &gt;=  Fecha_Inicio_2__c  || Fecha_Fin_2__c &gt;=  Fecha_Inicio_3__c || Fecha_Fin_3__c &gt;=  Fecha_Inicio_4__c || Fecha_Fin_4__c &gt;=  Fecha_Inicio_5__c || Fecha_Fin_5__c &gt;=  Fecha_Inicio_6__c</errorConditionFormula>
        <errorMessage>La fecha de fin de un periodo no puede ser más grande que la fecha de inicio del siguiente periodo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fechas_Importes_2</fullName>
        <active>true</active>
        <errorConditionFormula>Fecha_Inicio_1__c &gt;  Fecha_Fin_1__c || Fecha_Inicio_2__c &gt;  Fecha_Fin_2__c || Fecha_Inicio_3__c &gt;  Fecha_Fin_3__c || Fecha_Inicio_4__c &gt;  Fecha_Fin_4__c || Fecha_Inicio_5__c &gt;  Fecha_Fin_5__c || Fecha_Inicio_6__c &gt;  Fecha_Fin_6__c</errorConditionFormula>
        <errorMessage>La fecha de inicio de un periodo no puede ser posterior que la fecha de fin de ese mismo periodo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdContrato_no_valido</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Id_contrato__c  , &quot;[C]{1}[T]{1}[R]{1}[0-9]{5}&quot;) ) &amp;&amp;  NOT(  ISBLANK(Id_contrato__c ) )</errorConditionFormula>
