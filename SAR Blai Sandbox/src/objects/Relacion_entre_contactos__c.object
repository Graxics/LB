    <validationRules>
        <fullName>Informar_tipo_de_beneficiario</fullName>
        <active>true</active>
        <description>Informar sólo un tipo de beneficiario</description>
        <errorConditionFormula>(RecordType.DeveloperName =&apos;Relacion_de_beneficiarios&apos;  &amp;&amp;  (Usuario_principal__c =false  &amp;&amp;
Beneficiario_con_colgante__c =false  &amp;&amp;  Beneficiario_sin_colgante__c =false &amp;&amp; Pagador__c  =false)
|| 
(Usuario_principal__c =true  &amp;&amp;
Beneficiario_con_colgante__c =true  &amp;&amp;  Beneficiario_sin_colgante__c =true)
|| 
(Usuario_principal__c =true  &amp;&amp;
Beneficiario_con_colgante__c =true  &amp;&amp;  Beneficiario_sin_colgante__c =false)
|| 
(Usuario_principal__c =false  &amp;&amp;
Beneficiario_con_colgante__c =true  &amp;&amp;  Beneficiario_sin_colgante__c =true)
|| 
(Usuario_principal__c =true  &amp;&amp;
Beneficiario_con_colgante__c =false  &amp;&amp;  Beneficiario_sin_colgante__c =true))
&amp;&amp;
$Profile.Name  &lt;&gt;&apos;Administrador del Sistema (force.com)&apos;</errorConditionFormula>
        <errorMessage>Sólo se permite informar un tipo de beneficiario</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_modificar_relaciones_exitentes</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName=&apos;Relacion_de_beneficiarios&apos;  &amp;&amp;  NOT(ISNEW())  &amp;&amp; (( ISPICKVAL( Oportunidad__r.Etapa__c , &apos;Cerrada Ganada&apos;) )  || ISPICKVAL( Oportunidad__r.Etapa__c , &apos;Cerrada Perdida&apos;)) &amp;&amp; $Profile.Name  &lt;&gt;&apos;Administrador del Sistema (force.com)&apos;</errorConditionFormula>
        <errorMessage>No se pueden modificar relaciones de beneficiarios existentes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Residente_no_puede_ser_Contacto</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Contacto__c ))  &amp;&amp;  NOT(ISBLANK( Residente__c )) &amp;&amp;   Contacto__r.Id = Residente__r.Id &amp;&amp; $Profile.Name  &lt;&gt;&apos;Administrador del Sistema (force.com)&apos;</errorConditionFormula>
        <errorMessage>El residente y el contacto no pueden ser la misma persona</errorMessage>
    </validationRules>
